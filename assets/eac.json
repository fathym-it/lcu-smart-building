{
  "Applications": {
    "$1": {
      "Application": {
        "Name": "IoT Ensemble API Proxy",
        "Description": "The API Proxy for IoT Ensemble APIs."
      },
      "LookupConfig": {
        "PathRegex": "/api/demos/iot-ensemble.*"
      },
      "LowCodeUnit": {
        "APIRoot": "https://fathym-cloud-prd.azure-api.net/fcp-iotensemble",
        "Security": "lcu-subscription-key~{{IOT_ENSEMBLE_API_KEY}}",
        "Type": "API"
      },
      "Processor": {
        "InboundPath": "api/demos/iot-ensemble",
        "Type": "Proxy"
      }
    },
    "$2": {
      "Application": {
        "Name": "Habistack API Proxy",
        "Description": "The API Proxy for Habistack APIs."
      },
      "LookupConfig": {
        "PathRegex": "/api/demos/habistack.*"
      },
      "LowCodeUnit": {
        "APIRoot": "https://fathym-cloud-prd.azure-api.net/habistack/weather/ground",
        "Security": "lcu-subscription-key~{{HABISTACK_API_KEY}}",
        "Type": "API"
      },
      "Processor": {
        "InboundPath": "api/demos/habistack",
        "Type": "Proxy"
      }
    },
    "$3": {
      "Application": {
        "Name": "Geocodio API Proxy",
        "Description": "The API Proxy for Geocodio APIs."
      },
      "LookupConfig": {
        "PathRegex": "/api/demos/geocodio.*"
      },
      "LowCodeUnit": {
        "APIRoot": "https://api.geocod.io/v1.7/",
        "Security": "?api_key~{{GEOCODIO_API_KEY}}",
        "Type": "API"
      },
      "Processor": {
        "InboundPath": "api/demos/geocodio",
        "Type": "Proxy"
      }
    },
    "$4": {
      "Application": {
        "Description": "A starter for custom smart buildings apps that utilizes IoT and Weather data.",
        "Name": "Smart Building Demo Starter - Forked Recipe"
      },
      "LookupConfig": {
        "PathRegex": "/demos/smart-building.*"
      },
      "LowCodeUnit": {
        "SourceControlLookup": "fork|github://fathym-it/smart-building-demo",
        "Build": "latest",
        "Path": "lcu-github-artifacts-release.yml",
        "Type": "GitHub"
      },
      "Processor": {
        "BaseHref": "/demos/smart-building/",
        "DefaultFile": "index.html",
        "Type": "DFS"
      },
      "DataTokens": {
        "lcu-state-config": {
          "Name": "state-config",
          "Description": "state-config",
          "Value": "{\n  \"PointAPIQuery\": \"/api/v0/point-query\",\n  \"VariablesAPIQuery\": \"/api/v0/api-variables\",\n  \"HabistackAPIRoot\": \"/api/demos/habistack\",\n  \"GeocodioAPIRoot\": \"/api/demos/geocodio\",\n  \"GeocodioQuery\": \"/geocode\",\n  \"IoTAPIQuery\": \"/WarmQuery?includeEmulated=true&page=1&pageSize=100\",\n  \"IoTAPIRoot\": \"/api/demos/iot-ensemble\",\n  \"SelectedVariables\": [\n    \"WindGust_Surface\",\n    \"WindSpeed_10Meters\",\n    \"WindDirection_10Meters\",\n \"Temperature_Surface\"\n ],\n \"DefaultChartPrefs\": [\n { \n \"Mode\": \"Light\",\n \"ChartType\": \"Line\",\n \"BackgroundColor\": \"rgb(0, 0, 0, 0.4)\", \n \"BorderColor\": \"rgb(0, 0, 0, 0.4)\" \n }, \n { \n \"Mode\": \"Dark\",\n \"ChartType\": \"Line\",\n \"BackgroundColor\": \"rgb(255, 255, 255, 0.4)\", \n \"BorderColor\": \"rgb(255, 255, 255, 0.4)\" \n }, \n { \n \"Mode\": \"Light\",\n \"ChartType\": \"Bar\",\n \"BackgroundColor\": \"rgb(0, 0, 0, 0.4)\", \n \"BorderColor\": \"rgb(0, 0, 0, 0.4)\" \n }, \n { \n \"Mode\": \"Dark\",\n \"ChartType\": \"Bar\",\n \"BackgroundColor\": \"rgb(255, 255, 255, 0.4)\", \n \"BorderColor\": \"rgb(255, 255, 255, 0.4)\" \n } \n ], \n \"Location\": \"Denver, CO\",\n  \"RefreshRate\": \"30000\"\n}"
        }
      }
    }
  },
  "Environments": {
    "$#.PrimaryEnvironment": {
      "Artifacts": {
        "react-build-release": {
          "Name": "React Build/Release",
          "Output": "./build",
          "Type": "GitHub",
          "BuildCommand": "npm run build",
          "InstallCommand": "npm ci",
          "NPMRegistry": "https://registry.npmjs.org/"
        }
      },
      "DevOpsActions": {
        "github-artifacts-release-react": {
          "Name": "GitHub Artifacts - Release - React",
          "Path": "lcu-github-artifacts-release.yml",
          "ArtifactLookups": ["react-build-release"],
          "Templates": [
            "https://raw.githubusercontent.com/lowcodeunit/infrastructure/master/github/actions/artifacts/_header.hbs.yml",
            "https://raw.githubusercontent.com/lowcodeunit/infrastructure/master/github/actions/artifacts/release-flow.yml"
          ]
        }
      },
      "Sources": {
        "fork|github://fathym-it/smart-building-demo": {
          "DevOpsActionTriggerLookups": ["github-artifacts-release-react"],
          "Branches": ["main"],
          "MainBranch": "main",
          "Organization": "fathym-it",
          "Repository": "smart-building-demo",
          "Name": "github://fathym-it/smart-building-demo",
          "Type": "GitHub"
        }
      }
    }
  }
}
